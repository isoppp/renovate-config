
{
  "extends": ["config:recommended", "group:allNonMajor", ":timezone(Asia/Tokyo)"],
  "schedule": ["every weekend"],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 10,
  "labels": ["renovate"],
  "additionalBranchPrefix": "{{parentDir}}-",
  "rangeStrategy": "pin",
  "packageRules": [
    {
      "matchPackageNames": ["node", "postgres", "mysql"],
      "enabled": false
    }
  ]
}
