
{
  "extends": ["config:best-practices", "group:allNonMajor"],
  "timezone": "Asia/Tokyo",
  "schedule": ["after 7pm on Friday"],
  "prHourlyLimit": 0,
  "prConcurrentLimit": 10,
  "labels": ["renovate"],
  "additionalBranchPrefix": "{{packageFileDir}}-",
  "rangeStrategy": "pin",
  "packageRules": [
    {
      "matchPackageNames": ["node", "postgres", "mysql"],
      "enabled": false
    }
  ]
}
